<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vyommaitreya.android.scienceup">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon"
        android:supportsRtl="true"
        android:theme="@style/AppThemeTwo">
        <activity
            android:name=".activities.Academics"
            android:label="@string/title_activity_academics"
            android:parentActivityName=".activities.Dashboard"
            android:theme="@style/AppThemeTwo.NoActionBar" />
        <activity
            android:name=".activities.Timetable"
            android:label="@string/title_activity_timetable"
            android:parentActivityName=".activities.Dashboard"
            android:theme="@style/AppThemeTwo.NoActionBar" />
        <activity
            android:name=".activities.Chatroom"
            android:label="@string/title_activity_chatroom"
            android:parentActivityName=".activities.Dashboard"
            android:theme="@style/AppThemeTwo.NoActionBar" />
        <activity
            android:name=".activities.Login"
            android:label="@string/app_name"
            android:theme="@style/AppThemeTwo">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.Dashboard"
            android:label="@string/app_name"
            android:theme="@style/AppThemeTwo.NoActionBar" />
        <activity
            android:name=".activities.StudyMaterial"
            android:label="@string/title_activity_study_material"
            android:parentActivityName=".activities.Dashboard"
            android:theme="@style/AppThemeTwo.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.vyommaitreya.android.scienceup.activities.Dashboard" />
        </activity>
        <activity
            android:name=".activities.Settings"
            android:label="@string/title_activity_settings"
            android:parentActivityName=".activities.Dashboard"
            android:theme="@style/AppThemeTwo.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.vyommaitreya.android.scienceup.activities.Dashboard" />
        </activity>
        <activity
            android:name=".activities.Attendance"
            android:label="@string/title_activity_attendance"
            android:parentActivityName=".activities.Dashboard"
            android:theme="@style/AppThemeTwo.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.vyommaitreya.android.scienceup.activities.Dashboard" />
        </activity>
        <activity
            android:name=".activities.Feedback"
            android:label="@string/title_activity_feedback"
            android:parentActivityName=".activities.Dashboard"
            android:theme="@style/AppThemeTwo.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.vyommaitreya.android.scienceup.activities.Dashboard" />
        </activity>
        <activity
            android:name=".activities.Trends"
            android:label="@string/title_activity_trends"
            android:parentActivityName=".activities.Dashboard"
            android:theme="@style/AppThemeTwo.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.vyommaitreya.android.scienceup.activities.Dashboard" />
        </activity>
        <activity
            android:name=".activities.CampusRadio"
            android:label="@string/title_activity_campus_radio"
            android:parentActivityName=".activities.Dashboard"
            android:theme="@style/AppThemeTwo.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.vyommaitreya.android.scienceup.activities.Dashboard" />
        </activity>
    </application>

</manifest>